<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>sebescudie</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/sebescudie/VL.PluginTemplate</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/sebescudie/VL.PluginTemplate</PackageProjectUrl>
    <PackageId>VL.PluginTemplate</PackageId>
    <Version>6.0.10</Version>
    <Description>Template for a VL plugin</Description>
    <Copyright>sebescudie 2020</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\*.vl">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\..\**\*.vl">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
